{
  "name": "image-marketplace",
  "version": "1.0.0",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "knex migrate:latest",
    "rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "migrateh": "heroku run knex migrate:latest -a imageforsale",
    "rollbackh": "heroku run knex migrate:rollback -a imageforsale",
    "databaseh": "heroku pg:psql -a imageforsale",
    "seedh": "heroku run knex seed:run -a imageforsale",
    "test": "cross-env NODE_ENV=testing jest --watchAll --verbose --runInBand",
    "docker-postbuild":"cd client && npm i && npm run build && cd .. && npm i",
    "heroku-postbuild": "cd client && npm i && npm run build",
    "railway-postbuild": "cd client && npm i && npm run build",
    "railwayPostBuild": "cd client && npm i && npm run build",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "18.16.0",
    "npm": "9.5.1"
  },
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^4.0.2",
    "aws-sdk": "^2.1536.0",
    "axios": "^1.6.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "connect-session-knex": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.4.3",
    "express-session": "^1.17.2",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^3.1.2",
    "knex": "^3.1.0",
    "knex-cleaner": "^1.3.1",
    "pg": "8.7.1",
    "sharp": "^0.33.2",
    "stripe": "^14.12.0",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "27.0.1",
    "cross-env": "7.0.3",
    "eslint": "7.32.0",
    "jest": "26.0.0",
    "nodemon": "^2.0.12",
    "supertest": "6.1.6"
  }
}
